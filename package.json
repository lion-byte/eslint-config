{
  "name": "@lionbyte/eslint-config",
  "version": "1.8.0",
  "description": "ESLint config for personal projects",
  "scripts": {
    "format": "npm run prettier && npm run lint -- --fix",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|yml|yaml|css|less|ts|md|graphql|mdx)\"",
    "start": "node .",
    "test": "npm run lint && jest --coverage",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "main": "index.js",
  "author": "Mark Hernandez <mark@lion-byte.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/lion-byte/eslint-config.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "semantic-release": "^17.0.4"
  },
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "eslint-plugin-standard": "^4.0.1"
  },
  "peerDependencies": {
    "eslint": ">= 6.6.0"
  }
}
