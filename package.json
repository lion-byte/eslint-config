{
  "name": "@lionbyte/eslint-config",
  "version": "3.0.0-next.1",
  "description": "ESLint config for personal projects",
  "scripts": {
    "format": "npm run prettier && npm run lint -- --fix",
    "lint": "eslint",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(json|yml|yaml|css|less|md|graphql|mdx)\"",
    "start": "node .",
    "test": "npm run lint && node --test",
    "semantic-release": "semantic-release",
    "prepare": "husky"
  },
  "license": "MIT",
  "author": "Mark Hernandez <mark@lion-byte.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/lion-byte/eslint-config.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "default": "./index.js"
    }
  },
  "engines": {
    "node": ">=22.21.1"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "semantic-release": "^25.0.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1"
  },
  "peerDependencies": {
    "@eslint/js": "^10",
    "eslint": "^10",
    "eslint-config-prettier": "^10",
    "eslint-plugin-prettier": "^5",
    "globals": "^17",
    "prettier": "^3",
    "typescript": "^5",
    "typescript-eslint": "^8"
  }
}
