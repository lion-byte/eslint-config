{
  "name": "@lionbyte/eslint-config",
  "version": "1.8.2",
  "description": "ESLint config for personal projects",
  "scripts": {
    "format": "npm run prettier && npm run lint -- --fix",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|yml|yaml|css|less|ts|md|graphql|mdx)\"",
    "start": "node .",
    "test": "npm run lint && jest --coverage",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "main": "index.js",
  "author": "Mark Hernandez <mark@lion-byte.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/lion-byte/eslint-config.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "eslint": "7.27.0",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "semantic-release": "^17.4.3"
  },
  "dependencies": {
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.4",
    "eslint": "^7.27.0"
  }
}
