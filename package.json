{
  "name": "@lionbyte/eslint-config",
  "version": "0.0.0-development",
  "description": "ESLint config for personal projects",
  "scripts": {
    "format": "npm run prettier && npm run lint -- --fix",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|yml|yaml|css|less|ts|md|graphql|mdx)\"",
    "start": "node .",
    "test": "npm run lint && jest --coverage",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "main": "index.js",
  "author": "Mark Hernandez <mark@lion-byte.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/lion-byte/eslint-config.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.5",
    "@semantic-release/git": "^7.0.18",
    "eslint": "^6.6.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "semantic-release": "^15.13.30"
  },
  "dependencies": {
    "babel-eslint": "^10.0.3",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "eslint-plugin-standard": "^4.0.1"
  },
  "peerDependencies": {
    "eslint": ">= 6.6.0"
  }
}
